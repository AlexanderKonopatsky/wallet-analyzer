# Cielo Finance API Keys
# Primary key (required)
CIELO_API_KEY=your_cielo_api_key_here

# Additional keys for rotation (optional, up to 99)
# Uncomment and add your keys if you have multiple
# CIELO_API_KEY_1=your_second_key
# CIELO_API_KEY_2=your_third_key
# CIELO_API_KEY_3=your_fourth_key

# OpenRouter API Key (required for AI analysis)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google OAuth (required for authentication)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# JWT Secret (required - generate a random string)
# Example: openssl rand -hex 32
JWT_SECRET=your_random_jwt_secret_here

# Analysis Settings (optional)
# Number of recent analyses to include in full context (default: 1)
FULL_CHRONOLOGY_COUNT=1

# Auto-classification settings
AUTO_CLASSIFY_ENABLED=false
AUTO_CLASSIFY_BATCH_SIZE=3

# Auto-refresh settings (optional)
# Enable automatic wallet refresh at scheduled time
AUTO_REFRESH_ENABLED=false
# Time to run auto-refresh in UTC (24-hour format, e.g., "23:00" for 11 PM UTC)
AUTO_REFRESH_TIME=23:00

# Context compression settings (optional)
CONTEXT_COMPRESSION_ENABLED=true
CONTEXT_DAILY_COUNT=30
CONTEXT_WEEKLY_COUNT=30
CONTEXT_TIER2_GROUP_SIZE=5
CONTEXT_TIER3_SUPER_SIZE=3

# Payment settings (1Click)
# Where incoming swaps should be delivered
RECEIVE_ADDRESS=your_receive_wallet_address_here
# Destination token in format chain:token (default is base:usdc)
RECEIVE_TOKEN=base:usdc
# Optional JWT token for 1Click API (if provider/account requires auth)
ONECLICK_JWT=

# Railway (auto-injected, don't set manually)
# PORT=8000
# RAILWAY_PUBLIC_DOMAIN=your-app.railway.app
