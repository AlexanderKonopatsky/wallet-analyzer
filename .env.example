# Cielo Finance API Keys
# Primary key (required)
CIELO_API_KEY=your_cielo_api_key_here

# Additional keys for rotation (optional, up to 99)
# Uncomment and add your keys if you have multiple
# CIELO_API_KEY_1=your_second_key
# CIELO_API_KEY_2=your_third_key
# CIELO_API_KEY_3=your_fourth_key

# OpenRouter API Key (required for AI analysis)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Analysis Settings (optional)
# Number of recent analyses to include in full context (default: 1)
FULL_CHRONOLOGY_COUNT=1

# Number of related wallets to classify in parallel (default: 3)
AUTO_CLASSIFY_BATCH_SIZE=3
